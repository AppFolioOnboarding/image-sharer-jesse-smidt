<h1 class="mt-3">Create a New Image</h1>
<h5>Add a new image url here</h5>
<br/>
<%= form_for @image do |f| %>
  <div class="form-group">
      <%= f.label :url, 'Image Url' %>
      <%= f.text_field :url, class: 'form-control col-md-4', placeholder:'https://appfolio.com' %>
  </div>
  <%= render partial: 'tag_edit', locals: { form: f, tag_list: @image.tag_list } %>
  <%= render partial: '_partials/validation_list', locals: { errors: @image.errors } %>
  <%= f.submit 'Create', {class: 'btn btn-primary'} %>
<% end %>
